class Solution {
public:
    bool isPalindrome(int x) {
        
        if (x<0|| (x % 10 == 0 && x!=10 )){
            return false;
        }
        int reserverd_num = 0;
        int orginal = x;
        while (x>0){
            int digit = x % 10 ;
            reserverd_num = reserverd_num *10 +digit;
            x/=10;
        }
        return orginal == reserverd_num;
    }
};
